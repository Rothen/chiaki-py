    "version": "0.2.0",
    "configurations": [{
            "name": "Debug C++ in Python Extension",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "C:/Users/benir/anaconda3/envs/chiaki-ng/python.exe",
            "args": [
                "${workspaceFolder}/test.py"
            ],
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PYTHONPATH",
                    "value": "${workspaceFolder}/build-debug"
                }
