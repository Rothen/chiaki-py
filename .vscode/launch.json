            "stopAtEntry": false,
            "console": "integratedTerminal",
            "logging": {
                "engineLogging": true,
                "moduleLoad": true
            },
            "symbolSearchPath": "${workspaceFolder}/build-debug",
            "externalConsole": false
        }, {
            "name": "Launch Qt Debug App",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "chiaki.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/libs/chiaki-ng/chiaki-ng-Win-debug",
            "environment": [],
            "console": "externalTerminal",
            "symbolSearchPath": "${workspaceFolder}/libs/chiaki-ng/build-portable-debug/gui",
            "logging": {
                "engineLogging": true
            },
            "externalConsole": false
        }]
